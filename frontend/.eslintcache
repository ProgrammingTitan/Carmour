ge';\nimport VehiclePage from './components/pages/VehiclePage';\n\nexport default function App() {\n\n    const [userData, setUserData] = useState({\n        token: undefined,\n        id: undefined,\n        user: undefined,\n        vehicles: []\n    })\n\n    useEffect(() => {\n        const checkLoggedIn = async () => {\n            let token = localStorage.getItem(\"auth-token\");\n            \n            if(token === null) {\n                localStorage.setItem(\"auth-token\", \"\");\n                token = \"\";\n            }\n            const tokenRes = await Axios.post(\"http://localhost:5000/api/users/tokenIsValid\", null, {headers: {\"x-auth-token\" : token}});\n\n            \n            if(tokenRes.data) {\n                const userRes = await Axios.get(\"http://localhost:5000/api/users/\",  {headers: {\"x-auth-token\" : token}});\n\n                // setUserData({\n                //     token,\n                //     user: userRes.data \n                // })\n               \n\n                const vehicleRes = await Axios.get(\"http://localhost:5000/api/vehicles/\", {headers: {\"x-auth-token\":token}});\n                setUserData({\n                    token,\n                    user: userRes.data,\n                    vehicles: vehicleRes.data \n                });\n\n                console.log(vehicleRes.data);\n                console.log(userData.vehicles);\n\n            }\n        };\n\n        checkLoggedIn();\n    }, []);\n\n    return (\n        <>\n        <BrowserRouter>\n        <UserContext.Provider value={{userData, setUserData}}>\n        < Header />\n        <div >\n        <Switch>\n            <Route\n                path=\"/\" exact\n                component= {Home}\n            />\n            <Route\n                path=\"/Mission\" \n                component= {AboutUs}\n            />\n            <Route\n                path=\"/About\" \n                component= {AboutTeam}\n            />\n            <Route\n                path=\"/Login\" \n                component= {Login}\n            />\n            <Route\n                path=\"/Register\" \n                component= {Register}\n            />\n            <Route\n                path=\"/MyVehicles/:id\" \n                component= {MyVehiclesPage}\n            />\n            <Route\n                path=\"/Vehicle/:id\" \n                render={ (match) => <VehiclePage \n                    match = {match}\n                   \n            />\n                }\n                />\n            <Route\n                path=\"/accountOptions/:id\" \n                render={ (match) => <AccountOptions \n                    token = {userData.token}\n                    match = {match}\n                   \n                />\n                }\n                />\n                <Route \n                    path=\"/Menu\"\n                    component ={AboutUs}\n                />\n        </Switch>\n        </div>\n        </UserContext.Provider>\n        </BrowserRouter>\n        <Footer />\n        </>\n    )\n}\n","C:\\Users\\paval\\Documents\\WebDev\\Carmour\\frontend\\src\\components\\Router.js",[],"C:\\Users\\paval\\Documents\\WebDev\\Carmour\\frontend\\src\\CreateProfile.js",["172","173","174"],"// /client/App.js\r\nimport React, { Component } from 'react';\r\nimport axios from 'axios';\r\nimport SMSForm from './SMSForm';\r\nimport Navit from './components/Nav';\r\nclass CreateProfile extends Component {\r\n  // initialize our state\r\n  state = {\r\n    data: [],\r\n    id: 0,\r\n    message: null,\r\n    username: null,\r\n    fullname: null,\r\n    intervalIsSet: false,\r\n    idToDelete: null,\r\n    usernameToUpdate: null,\r\n    passwordToUpdate: null,\r\n    objectToUpdate: null,\r\n  };\r\n\r\n  // when component mounts, first thing it does is fetch all existing data in our db\r\n  // then we incorporate a polling logic so that we can easily see if our db has\r\n  // changed and implement those changes into our UI\r\n  componentDidMount() {\r\n    this.getDataFromDb();\r\n    if (!this.state.intervalIsSet) {\r\n      let interval = setInterval(this.getDataFromDb, 1000);\r\n      this.setState({ intervalIsSet: interval });\r\n    }\r\n  }\r\n\r\n  // never let a process live forever\r\n  // always kill a process everytime we are done using it\r\n  componentWillUnmount() {\r\n    if (this.state.intervalIsSet) {\r\n      clearInterval(this.state.intervalIsSet);\r\n      this.setState({ intervalIsSet: null });\r\n    }\r\n  }\r\n\r\n  // just a note, here, in the front end, we use the id key of our data object\r\n  // in order to identify which we want to Update or delete.\r\n  // for our back end, we use the object id assigned by MongoDB to modify\r\n  // data base entries\r\n\r\n  // our first get method that uses our backend api to\r\n  // fetch data from our data base\r\n  getDataFromDb = () => {\r\n    fetch('http://localhost:3001/api/getData')\r\n      .then((data) => data.json())\r\n      .then((res) => this.setState({ data: res.data }));\r\n  };\r\n\r\n  // our put method that uses our backend api\r\n  // to create new query into our data base\r\n  putDataToDB = (message,username,fullname,password) => {\r\n    let currentIds = this.state.data.map((data) => data.id);\r\n    let idToBeAdded = 0;\r\n    while (currentIds.includes(idToBeAdded)) {\r\n      ++idToBeAdded;\r\n    }\r\n\r\n    axios.post('http://localhost:3001/api/putData', {\r\n      id: idToBeAdded,\r\n      message: message,\r\n      username: username,\r\n      fullname: fullname,\r\n      password: password\r\n    });\r\n  };\r\n\r\n // our delete method that uses our backend api\r\n  // to remove existing database information\r\n  deleteFromDB = (idToDelete) => {\r\n    let objIdToDelete = null;\r\n    parseInt(idToDelete);\r\n    this.state.data.forEach((dat) => {\r\n      if (dat.id == idToDelete) {\r\n        objIdToDelete = dat._id;\r\n      }\r\n    });\r\n\r\n    axios.delete('http://localhost:3001/api/deleteData', {\r\n      data: {\r\n        id: objIdToDelete,\r\n      },\r\n    });\r\n  };\r\n\r\n  // our update method that uses our backend api\r\n  // to overwrite existing data base information\r\n  updateDB = (usernameToUpdate, passwordToUpdate, updateToApply) => {\r\n    let objIdToUpdate = null;\r\n    //parseInt(idToUpdate);\r\n    this.state.data.forEach((dat) => {\r\n      if ((dat.username === usernameToUpdate) && (dat.password === passwordToUpdate)) {\r\n        objIdToUpdate = dat._id;\r\n      }\r\n    });\r\n\r\n    axios.post('http://localhost:3001/api/updateData', {\r\n      id: objIdToUpdate,\r\n      update: { message: updateToApply },\r\n    });\r\n  };\r\n\r\n  // here is our UI\r\n  // it is easy to understand their functions when you\r\n  // see them render into our screen\r\n  render() {\r\n    const { data } = this.state;\r\n    return (\r\n      <div>\r\n        <Navit />\r\n        {/* <ul>\r\n          {data.length <= 0\r\n            ? 'NO DB ENTRIES YET'\r\n            : data.map((dat) => (\r\n                <li style={{ padding: '10px' }} key={data.message}>\r\n                  <span style={{ color: 'gray' }}> id: </span> {dat.id} <br />\r\n                  <span style={{ color: 'gray' }}> phone number: </span>\r\n                  {dat.message}\r\n                  <span style={{ color: 'gray' }}> username: </span>\r\n                  {dat.username}\r\n                  <span style={{ color: 'gray' }}> Full Name: </span>\r\n                  {dat.fullname}\r\n                </li>\r\n              ))}\r\n        </ul> */}\r\n        <div style={{ padding: '10px' }}>\r\n          <input\r\n            type=\"text\"\r\n            onChange={(e) => this.setState({ message: e.target.value })}\r\n            placeholder=\"Phone Number\"\r\n            style={{ width: '200px' }}\r\n          />\r\n          <input\r\n            type=\"text\"\r\n            onChange={(e) => this.setState({ username: e.targe